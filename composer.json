{
  "name": "wedocreatives/wrike-php-library",
  "description": "Wrike PHP Library, generic implementation.",
  "keywords": [
    "wrike",
    "wrike.com",
    "api",
    "sdk",
    "library",
    "client",
    "REST",
    "HTTP",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zbigniew Ślązak"
    }
  ],
  "require": {
    "php": ">=7.1",
    "psr/http-message": ">=1.0,<2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0,<8.0"
  },
  "autoload": {
    "psr-4": {
      "wedocreatives\\WrikePhpLibrary\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "wedocreatives\\WrikePhpLibrary\\Tests\\": "tests/"
    }
  }
}
